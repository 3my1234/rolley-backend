{
  "name": "rolley-backend",
  "version": "1.0.0",
  "description": "Rolley Backend API - NestJS",
  "author": "Rolley Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "migrate:deploy": "prisma migrate deploy",
    "prestart:dev": "npm run migrate:deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "deploy:compile": "hardhat compile",
    "deploy:show-address": "node scripts/show-wallet-address.js",
    "deploy:check-balance": "hardhat run scripts/check-balance.js --network amoy",
    "deploy:testnet": "hardhat run scripts/deploy-rolley-token.js --network amoy",
    "deploy:mainnet": "hardhat run scripts/deploy-rolley-token.js --network polygon",
    "deploy:authorize-minter": "hardhat run scripts/authorize-minter.js --network amoy",
    "test:mint": "hardhat run scripts/test-mint.js --network amoy",
    "verify:testnet": "hardhat verify --network amoy",
    "verify:mainnet": "hardhat verify --network polygon",
    "create:admin": "ts-node scripts/create-admin.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.10",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/websockets": "^10.2.10",
    "@prisma/client": "^5.19.1",
    "@privy-io/server-auth": "^1.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bloom-filters": "^3.0.4",
    "bullmq": "^5.63.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "ethers": "^6.14.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "nanoid": "^5.1.6",
    "passport": "^0.7.0",
    "passport-custom": "^1.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.19.1",
    "react-qr-code": "^2.0.18",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.26.5",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
